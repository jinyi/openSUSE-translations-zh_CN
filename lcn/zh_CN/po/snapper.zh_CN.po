#: ../tools/snapper.cc:464 ../tools/snapper.cc:622
msgid "\t--cleanup-algorithm, -c <algo>\tCleanup algorithm for snapshot."
msgstr "\t--cleanup-algorithm, -c <算法> \t快照的清空算法。"

#: ../tools/snapper.cc:466
msgid "\t--command <command>\tRun command and create pre and post snapshots."
msgstr "\t--command <命令> \t运行命令，创建前后快照。"

#: ../tools/snapper.cc:1076
msgid "\t--config, -c <name>\t\tSet name of config to use."
msgstr "\t--config, -c <名称> \t\t设定要使用的配置名"

#: ../tools/snapper.cc:463 ../tools/snapper.cc:621
msgid "\t--description, -d <description>\tDescription for snapshot."
msgstr "\t--description, -d <描述> \t快照描述。"

#: ../tools/snapper.cc:1077
msgid "\t--disable-filters\t\tDisable filters."
msgstr "\t--disable-filters \t\t禁用过滤器。"

#: ../tools/snapper.cc:222
msgid "\t--fstype, -f <fstype>\t\tManually set filesystem type."
msgstr "\t--fstype, -f <文件系统类型> \t\t手动设定文件系统类型。"

#: ../tools/snapper.cc:899
msgid "\t--input, -i <file>\t\tRead files for which to undo changes from file."
msgstr "\t--input, -i <文件> \t\t读取要回滚的文件。"

#: ../tools/snapper.cc:794
msgid "\t--output, -o <file>\t\tSave status to file."
msgstr "\t--output, -o <文件> \t\t保存状态到文件。"

#: ../tools/snapper.cc:461
msgid "\t--pre-number <number>\t\tNumber of corresponding pre snapshot."
msgstr "\t--pre-number <编号> \t\t相关的预快照编号。"

#: ../tools/snapper.cc:462
msgid "\t--print-number, -p\t\tPrint number of created snapshot."
msgstr "\t--print-number, -p \t\t打印创建的快照的编号。"

#: ../tools/snapper.cc:1073
msgid "\t--quiet, -q\t\t\tSuppress normal output."
msgstr "\t--quiet, -q\t\t\t压制正常输出．"

#: ../tools/snapper.cc:1075
msgid "\t--table-style, -t <style>\tTable style (integer)."
msgstr "\t--table-style, -t <样式> \t表格类型（整数）．"

#: ../tools/snapper.cc:223
msgid "\t--template, -t <name>\t\tName of config template to use."
msgstr "\t--template, -t <名称> \t\t要使用的配置模板名称．"

#: ../tools/snapper.cc:460
msgid "\t--type, -t <type>\t\tType for snapshot."
