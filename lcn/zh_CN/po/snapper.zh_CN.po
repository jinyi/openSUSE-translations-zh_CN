#: ../tools/snapper.cc:318
msgid "\t--type, -t <type>\t\tType of snapshots to list."
msgstr "\t--type, -t <类型> \t\t要列示的快照类型．"

#: ../tools/snapper.cc:465 ../tools/snapper.cc:623
msgid "\t--userdata, -u <userdata>\tUserdata for snapshot."
