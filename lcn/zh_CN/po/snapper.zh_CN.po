#: ../tools/snapper.cc:1074
msgid "\t--verbose, -v\t\t\tIncrease verbosity."
msgstr "\t--verbose, -v\t\t\t显示调试信息。"

#: ../tools/snapper.cc:1078
msgid "\t--version\t\t\tPrint version and exit."
msgstr "\t--version\t\t\t打印版本号并退出。"

